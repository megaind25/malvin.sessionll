{
  "name": "malvin-sessionll",
  "version": "1.0.0",
  "description": "Combined Node.js + Next.js Project",
  "main": "index.js",
  "engines": {
    "npm": ">=9.7.2",
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node index.js",                          // Node.js server
    "frontend-dev": "next dev -p 3001",               // Next.js on port 3001
    "frontend-build": "next build",                   // Next.js build
    "frontend-start": "next start -p 3001",           // Next.js production
    "combined-dev": "concurrently \"npm run start\" \"npm run frontend-dev\"",
    "combined-prod": "concurrently \"npm run start\" \"npm run frontend-start\""
  },
  "author": "kingmalvn",
  "license": "MIT",
  "dependencies": {
    // Existing Dependencies
    "@whiskeysockets/baileys": "^6.5.0",
    "maher-zubair-baileys": "^6.6.5",
    "qrcode": "^1.5.3",
    "awesome-phonenumber": "^2.64.0",
    "pino": "^8.1.0",
    "phone": "3.1.30",
    "body-parser": "^1.20.1",
    "pastebin-js": "^1.0.6",
    "express": "^4.18.1",
    "path": "^0.12.7",
    
    // New Dependencies
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "concurrently": "^8.2.2"
  }
}
